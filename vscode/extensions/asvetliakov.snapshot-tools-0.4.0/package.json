{
	"name": "snapshot-tools",
	"description": "Helpful tools for snapshot testing",
	"author": "Alexey Svetliakov <asvetliakov@teledini.com>",
	"license": "MIT",
	"version": "0.4.0",
	"publisher": "asvetliakov",
	"repository": {
		"type": "git",
		"url": "https://github.com/asvetliakov/typescript-snapshots-plugin"
	},
	"engines": {
		"vscode": "^1.8.0"
	},
	"icon": "icon.png",
	"categories": [
		"Programming Languages",
		"Other"
	],
	"contributes": {
		"languages": [
			{
				"id": "jest-snapshot",
				"extensions": [
					".js.snap",
					".jsx.snap",
					".ts.snap",
					".tsx.snap"
				]
			}
		],
		"grammars": [
			{
				"language": "jest-snapshot",
				"scopeName": "source.jest.snap",
				"path": "./syntaxes/jest-snapshot.tmLanguage"
			}
		],
		"typescriptServerPlugins": [
			{
				"name": "typescript-snapshots-plugin"
			}
		]
	},
	"dependencies": {
		"typescript-snapshots-plugin": "^1.7.0"
	},
	"__metadata": {
		"id": "b80129ef-d190-4ffd-80bf-7ebd643d53d1",
		"publisherId": "ce6190db-6762-4c9c-99c7-1717b9504159",
		"publisherDisplayName": "Alexey Svetliakov"
	}
}